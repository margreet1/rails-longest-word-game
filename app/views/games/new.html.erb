<h1>Games#new</h1>
<p>Find me in app/views/games/new.html.erb</p>

<%= @letters %>

<form action="score" >
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= hidden_field_tag :letters, @letters %>
  <input type="text" name="input">
  <input type="submit">
</form>


<!-- authenticity token is to make sure the data is sent from the website. Use for APIs etc., verification method. To protect from cross site request forgery -->
